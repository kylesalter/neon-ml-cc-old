  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty",
    "format": "run-p format:*",
    "format:doctoc": "doctoc --maxlevel 3 --notitle ./README.md",
    "format:imports": "import-sort --write '*.{js,jsx,ts,tsx}'",
    "format:package-json": "sort-package-json ./package.json ",
    "format:prettier": "prettier --write '**/*.{css,gql,graphql,html,js,jsx,json,less,md,mdx,scss,ts,tsx,vue,yaml,yml}' '.editorconfig' 'LICENSE'",
    "format:src": "yarn lint:fix",
    "lint": "run-p lint:js lint:styles lint:md",
    "lint:fix": "run-s lint:*:fix",
    "lint:js": "yarn eslint ./",
    "lint:js:fix": "yarn eslint --fix ./",
    "lint:js:watch": "esw --watch --clear --ext .js,.jsx,.ts,.tsx --format=pretty --color 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:md": "markdownlint --ignore node_modules './**/*.md'",
    "lint:md:watch": "onchange '**/*.md' --initial --kill --delay 1000  -- yarn lint:md",
    "lint:staged": "lint-staged",
    "lint:styles": "yarn stylelint './src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "lint:styles:fix": "yarn stylelint --fix './src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "lint:styles:watch": "onchange 'src/**/*.{js,jsx,ts,tsx,css,scss}' --initial --kill --delay 1000  -- yarn lint:styles",
    "lint:watch": "onchange 'src/**/*' --initial --kill --delay 1000  -- yarn lint",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "stylelint": "stylelint",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "typecheck": "yarn tsc --project ./tsconfig.json --noEmit --pretty",
    "typecheck:watch": "yarn typecheck --watch "
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint:staged build"
    }
  },
